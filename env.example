# FinTrust - Variáveis de Ambiente
# Copie este arquivo para .env.local e configure com suas credenciais

# QI Tech API Configuration
QI_TECH_API_URL=https://api.qitech.com.br
QI_TECH_API_KEY=sua_api_key_aqui

# NextAuth Configuration
NEXTAUTH_SECRET=seu_secret_super_seguro_aqui
NEXTAUTH_URL=http://localhost:3002

# Database Configuration (para produção)
DATABASE_URL=postgresql://user:pass@localhost:5432/fintrust

# Redis Configuration (para cache e sessões)
REDIS_URL=redis://localhost:6379

# Development Settings
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3002

# Security Settings
JWT_SECRET=seu_jwt_secret_aqui
ENCRYPTION_KEY=sua_chave_de_criptografia_aqui

# External APIs (opcional)
SPC_API_URL=https://api.spc.com.br
SERASA_API_URL=https://api.serasa.com.br
BANCO_CENTRAL_API_URL=https://api.bcb.gov.br

# Monitoring (opcional)
SENTRY_DSN=sua_sentry_dsn_aqui
ANALYTICS_ID=seu_google_analytics_id_aqui
